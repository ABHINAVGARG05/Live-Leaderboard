{
  "name": "live-leaderboard",
  "version": "1.0.4",
  "description": "A plug-and-play real-time leaderboard SDK using Redis and Socket.IO with Express router integration for easy backend setup.",
  "main": "dist/sdk/index.js",
  "types": "dist/sdk/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node playground/server.ts",
    "build": "tsc",
    "prepare": "pnpm run build",
    "dev": "ts-node-dev --respawn playground/server.ts"
  },
  "keywords": [
    "leaderboard",
    "redis",
    "socket.io",
    "express",
    "typescript",
    "sdk",
    "real-time"
  ],
  "author": "Abhinav Garg",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@types/pg": "^8.15.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/sdk/index.d.ts",
      "default": "./dist/sdk/index.js"
    },
    "./router": {
      "types": "./dist/sdk/router.d.ts",
      "default": "./dist/sdk/router.js"
    },
    "./leaderboard": {
      "types": "./dist/sdk/leaderboard/index.d.ts",
      "default": "./dist/sdk/leaderboard/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
